package com.isa.SW.entities;

/**
 *
 * @author JMiraballes
 */
public class User {
    
    private String description;
    private String iName;
    private String language;
    private String cNameFName;    
    private String cNameSurname;
    private String cNameTitle;
    private String oNameTitle;
    private String oNameOU;
    private String oNameO;
    private String sNameUID;
    private String SNamePasswd;
    private String ContactsWorkMail;
    private String ContactsWorkPhone;
    private String ContactsHomeMail;
    private String ContactsHomePhone;
    
    
    /**
     * Constructor para inicializar todas las propiedades, para que
     * ninguna este en null
     */
    public User(){
        this.description =          "";
        this.iName =                "";
        this.language =             "";
        this.cNameFName =           "";    
        this.cNameSurname =         "";
        this.cNameTitle =           "";
        this.oNameTitle =           "";
        this.oNameOU =              "";
        this.oNameO =               "";
        this.sNameUID =             "";
        this.SNamePasswd =          "";
        this.ContactsWorkMail =     "";
        this.ContactsWorkPhone =    "";
        this.ContactsHomeMail =     "";
        this.ContactsHomePhone =    "";
    }
    
    //Getters - Setters

    public String getDescription() {
        return description;
    }

    public void setDescription(String description) {
        this.description = description;
    }

    public String getiName() {
        return iName;
    }

    public void setiName(String iName) {
        this.iName = iName;
    }

    public String getLanguage() {
        return language;
    }

    public void setLanguage(String language) {
        this.language = language;
    }

    public String getcNameFName() {
        return cNameFName;
    }

    public void setcNameFName(String cNameFName) {
        this.cNameFName = cNameFName;
    }

    public String getcNameSurname() {
        return cNameSurname;
    }

    public void setcNameSurname(String cNameSurname) {
        this.cNameSurname = cNameSurname;
    }

    public String getcNameTitle() {
        return cNameTitle;
    }

    public void setcNameTitle(String cNameTitle) {
        this.cNameTitle = cNameTitle;
    }

    public String getoNameTitle() {
        return oNameTitle;
    }

    public void setoNameTitle(String oNameTitle) {
        this.oNameTitle = oNameTitle;
    }

    public String getoNameOU() {
        return oNameOU;
    }

    public void setoNameOU(String oNameOU) {
        this.oNameOU = oNameOU;
    }

    public String getoNameO() {
        return oNameO;
    }

    public void setoNameO(String oNameO) {
        this.oNameO = oNameO;
    }

    public String getsNameUID() {
        return sNameUID;
    }

    public void setsNameUID(String sNameUID) {
        this.sNameUID = sNameUID;
    }

    public String getSNamePasswd() {
        return SNamePasswd;
    }

    public void setSNamePasswd(String SNamePasswd) {
        this.SNamePasswd = SNamePasswd;
    }

    public String getContactsWorkMail() {
        return ContactsWorkMail;
    }

    public void setContactsWorkMail(String ContactsWorkMail) {
        this.ContactsWorkMail = ContactsWorkMail;
    }

    public String getContactsWorkPhone() {
        return ContactsWorkPhone;
    }

    public void setContactsWorkPhone(String ContactsWorkPhone) {
        this.ContactsWorkPhone = ContactsWorkPhone;
    }

    public String getContactsHomeMail() {
        return ContactsHomeMail;
    }

    public void setContactsHomeMail(String ContactsHomeMail) {
        this.ContactsHomeMail = ContactsHomeMail;
    }

    public String getContactsHomePhone() {
        return ContactsHomePhone;
    }

    public void setContactsHomePhone(String ContactsHomePhone) {
        this.ContactsHomePhone = ContactsHomePhone;
    }
}
